--changing supplier's names



merge into drug_concept_stage dcs
using
(select c.concept_name,c.concept_code from drug_concept_stage dcs2 join devv5.concept c on c.concept_code=dcs2.concept_code and c.vocabulary_id='RxNorm Extension' and c.concept_class_id='Supplier') c
on (dcs.concept_code=c.concept_code)
when matched then update
set dcs.concept_name=c.concept_name;

 update drug_concept_stage
 set concept_name=regexp_replace(concept_name,'Pharmaceuticals\.|Pharmaceuticals|Pharmaceutical Products|Pharmaceutical Product|Pharmaceutical|Pharmazeutische Produkte|Pharma|Pharms| Pharm$| Pharm ','',1,0,'i')
  where  lower(concept_name) like '% pharm%' and concept_class_id='Supplier';
  
 update drug_concept_stage
 set concept_name=regexp_replace(concept_name,'Incorporated|Inc\.|Inc')
  where  lower(concept_name) like '% inc%';
 
 update drug_concept_stage
 set concept_name=regexp_replace(concept_name,'GmbH & Co\. KG|GmbH & Co\.|GmbH & Co|GmbH|mbH & Co\.KG','',1,0,'i')
 where (lower(concept_name) like '% mbh%' or lower(concept_name) like '% gmbh%') and concept_class_id='Supplier';

 update drug_concept_stage
 set concept_name=regexp_replace(concept_name,'Group','',1,0,'i')
 where concept_class_id='Supplier' and concept_name not like '%Le Group%';
  
update drug_concept_stage
set concept_name=regexp_replace(concept_name,'Technologies|Limited|Specialty|Corporation|European|Europe| SERVICES|Manufacturing|Regulatory Solutions|Environmental Solutions|Mobility Solutions|Solutions|Vaccines and Diagnostics|Diagnostics|Life Sciences|Therapeutics|Deutschland|NETHERLANDS|TRADING|Medical Products|Medical','',1,0,'i')
where  concept_class_id='Supplier'
and not regexp_like(concept_name,'^Laboratories|^Healthcare|^Arzneimittel|^International|^Medical|^Product|^Nutrition|^Commercial');
update drug_concept_stage
set concept_name=trim(regexp_replace(regexp_replace(concept_name,'-$'),'  ',' '));

update drug_concept_stage
set concept_name=regexp_replace(concept_name,'Laboratories|Sweden|Healthcare Products|Healthcare|Commercial|Sales Services|Health Care|Management|Arzneimittel|Canada|Company|\(.*\)|Registration|Nutritionals|Nutrition|Consumer Health|Consumer|Sante| SCIENCES| MALTA|FRANCE|International| Inc\.?| Ltee| Ltd\.?| Plc| PLC| Llc| Ulc| UK| \(UK\)| \(U\.K\.\)|U\.K\.| EU$| Ab$| A/S| AG$|&$| and$| of$','',1,0,'i')
where  concept_class_id='Supplier'
and not regexp_like(concept_name,'^Laboratories|^Healthcare|^Arzneimittel|^International|^Medical|^Product|^Nutrition|^Commercial');

update drug_concept_stage
set concept_name=regexp_replace(concept_name,' \[HIST\]|e\.k\.','',1,0,'i') where concept_class_id='Supplier'
;
update DRUG_CONCEPT_STAGE
set concept_name=initcap(concept_name)
where concept_class_id='Supplier' and regexp_count (concept_name,'[[:upper:]]') >5;

update drug_concept_stage
set concept_name=trim(regexp_replace(regexp_replace(concept_name,'-$'),'  ',' '));


